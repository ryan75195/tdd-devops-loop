{
  "name": "Simple TDD Workflow",
  "description": "Simple TDD implementation example",
  "version": "2.0.0",
  "agents": [
    {
      "name": "tdd_only",
      "type": "tdd",
      "description": "TDD implementation for single work item",
      "max_iterations": 20,
      "parameters": {
        "project_path": ".",
        "work_item_id": "54321"
      }
    }
  ],
  "execution": {
    "type": "sequential",
    "agents": ["tdd_only"]
  },
  "outputs": [
    {
      "name": "tdd_implementation",
      "source": "tdd_only",
      "type": "json"
    }
  ]
}